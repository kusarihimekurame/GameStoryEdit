                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="LocationString">Ln:{0},Col:{1}</sys:String>
    <sys:String x:Key="Lines">{0} Lines</sys:String>
    <sys:String x:Key="Characters">{0} Characters</sys:String>
    <sys:String x:Key="SceneHeading">SceneHeading</sys:String>
    <sys:String x:Key="Character">Character</sys:String>
    <sys:String x:Key="Language">Language</sys:String>
    <sys:String x:Key="English">English</sys:String>
    <sys:String x:Key="Chinese(simplified)">Chinese(simplified)</sys:String>
    <sys:String x:Key="Japanese">Japanese</sys:String>
</ResourceDictionary>