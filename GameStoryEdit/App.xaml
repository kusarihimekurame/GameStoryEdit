<Application x:Class="GameStoryEdit.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GameStoryEdit"
             Startup="Application_Startup"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Language\en-US.xaml"/>
                <ResourceDictionary Source="Resources\Language\zh-CN.xaml"/>
                <ResourceDictionary Source="Resources\Language\ja-JP.xaml"/>
                <ResourceDictionary Source="Resources\Language\Languages.xaml"/>
                <ResourceDictionary Source="Resources\Command\Command.xaml"/>
                <ResourceDictionary Source="Resources\Style\MenuItem.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
